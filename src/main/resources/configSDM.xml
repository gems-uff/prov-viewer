<config
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xsi:noNamespaceSchemaLocation='ConfigSchema.xsd'>
    <layoutSpecialVertexType>Project</layoutSpecialVertexType>
    <edgetypes>
        <edge>Neutral</edge>
        <edgestroke>true</edgestroke>
        <collapsefunction>false</collapsefunction>
    </edgetypes>
    <edgetypes>
        <edge>Credits</edge>
        <edgestroke>false</edgestroke>
        <collapsefunction>false</collapsefunction>
    </edgetypes>
    <edgetypes>
        <edge>Quality</edge>
        <edgestroke>true</edgestroke>
        <collapsefunction>false</collapsefunction>
    </edgetypes>
    <edgetypes>
        <edge>Progress</edge>
        <edgestroke>true</edgestroke>
        <collapsefunction>false</collapsefunction>
    </edgetypes>
    <edgetypes>
        <edge>Aid</edge>
        <edgestroke>true</edgestroke>
        <collapsefunction>true</collapsefunction>
    </edgetypes>
    <edgetypes>
        <edge>Val</edge>
        <edgestroke>true</edgestroke>
        <collapsefunction>false</collapsefunction>
    </edgetypes>
    <edgetypes>
        <edge>Discovery</edge>
        <edgestroke>true</edgestroke>
        <collapsefunction>false</collapsefunction>
    </edgetypes>
    <edgetypes>
        <edge>Repair</edge>
        <edgestroke>true</edgestroke>
        <collapsefunction>false</collapsefunction>
    </edgetypes>
    <edgetypes>
        <edge>Bug</edge>
        <edgestroke>true</edgestroke>
        <collapsefunction>false</collapsefunction>
    </edgetypes>
    <edgetypes>
        <edge>TC</edge>
        <edgestroke>true</edgestroke>
        <collapsefunction>false</collapsefunction>
    </edgetypes>
    <edgetypes>
        <edge>Morale</edge>
        <edgestroke>true</edgestroke>
        <collapsefunction>false</collapsefunction>
    </edgetypes>
    <edgetypes>
        <edge>Stamina</edge>
        <edgestroke>true</edgestroke>
        <collapsefunction>false</collapsefunction>
    </edgetypes>
    <edgetypes>
        <edge>Prototype</edge>
        <edgestroke>true</edgestroke>
        <collapsefunction>false</collapsefunction>
    </edgetypes>
    <edgetypes>
        <edge>Negotiation</edge>
        <edgestroke>true</edgestroke>
        <collapsefunction>false</collapsefunction>
    </edgetypes>
    <colorscheme>
        <attribute>Morale</attribute>
        <class>AttributeMode</class>
        <values></values>
        <GreenThreshold></GreenThreshold>
        <YellowThreshold></YellowThreshold>
    </colorscheme>
    <colorscheme>
        <attribute>Stamina</attribute>
        <class>AttributeMode</class>
        <values></values>
        <GreenThreshold></GreenThreshold>
        <YellowThreshold></YellowThreshold>
    </colorscheme>
    <colorscheme>
        <attribute>Hours</attribute>
        <class>InvertedAttributeMode</class>
        <values></values>
        <GreenThreshold>6</GreenThreshold>
        <YellowThreshold>12</YellowThreshold>
    </colorscheme>
    <colorscheme>
        <attribute>Credits</attribute>
        <class>ConstantEntityAttributeMode</class>
        <values></values>
        <GreenThreshold></GreenThreshold>
        <YellowThreshold></YellowThreshold>
    </colorscheme>
    <colorscheme>
        <attribute>Role</attribute>
        <class>MultiAttributeMode</class>
        <values>Analyst Architect Manager Marketing Programmer Tester</values>
        <GreenThreshold></GreenThreshold>
        <YellowThreshold></YellowThreshold>
    </colorscheme>
    <colorscheme>
        <attribute>Weekend</attribute>
        <class>WeekendMode</class>
        <values>Sat Sun</values>
        <GreenThreshold></GreenThreshold>
        <YellowThreshold></YellowThreshold>
    </colorscheme>
    <vertexstroketype>
        <attribute>Role</attribute>
        <values>Idle Fired Promotion Training Hired"</values>
    </vertexstroketype>
    <activityVariables>
        <attribute>Task</attribute>
        <value>Idle</value>
        <vercolor>
            <r>238</r>
            <g>180</g>
            <b>180</b>
        </vercolor>
    </activityVariables>
    <activityVariables>
        <attribute>Role</attribute>
        <value>Training</value>
        <vercolor>
            <r>102</r>
            <g>0</g>
            <b>102</b>
        </vercolor>
    </activityVariables>
    <activityVariables>
        <attribute>Task</attribute>
        <value>Fired</value>
        <vercolor>
            <r>139</r>
            <g>69</g>
            <b>19</b>
        </vercolor>
    </activityVariables>
    <activityVariables>
        <attribute>Task</attribute>
        <value>Promotion</value>
        <vercolor>
            <r>0</r>
            <g>153</g>
            <b>0</b>
        </vercolor>
    </activityVariables>
    <activityVariables>
        <attribute>Task</attribute>
        <value>Hired</value>
        <vercolor>
            <r>139</r>
            <g>136</g>
            <b>120</b>
        </vercolor>
    </activityVariables>
    <activityVariables>
        <attribute>Task</attribute>
        <value>Negotiation</value>
        <vercolor>
            <r>193</r>
            <g>205</g>
            <b>193</b>
        </vercolor>
    </activityVariables>

</config>
