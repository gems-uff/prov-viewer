<config
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xsi:noNamespaceSchemaLocation='config.xsd'>
    <layoutbackbone>Player</layoutbackbone>
	<layoutAxis_X>ObjectPosition_X</layoutAxis_X>
	<layoutAxis_Y>ObjectPosition_Z</layoutAxis_Y>
	<imageLocation></imageLocation>
	<imageOffset_X>0.55</imageOffset_X>
	<imageOffset_Y>0.81</imageOffset_Y>
	<coordinatesLayoutPosition>10</coordinatesLayoutPosition>
	<temporalLayoutscale>0.5</temporalLayoutscale>
	<showentitydate>false</showentitydate>
    <edgetype>
        <edge>Neutral</edge>
        <edgestroke>MAX</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
	<edgetype>
        <edge>None</edge>
        <edgestroke>MAX</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
	<edgetype>
        <edge>Health (Player)</edge>
        <edgestroke>MEAN</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
	<edgetype>
        <edge>Health (Enemy)</edge>
        <edgestroke>MEAN</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
	<edgetype>
        <edge>Score</edge>
        <edgestroke>MEAN</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
	<edgetype>
        <edge>Health</edge>
        <edgestroke>MEAN</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
	<edgetype>
        <edge>Bomb</edge>
        <edgestroke>MEAN</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
    <colorscheme>
        <attribute>Health</attribute>
        <class>ActivityScheme</class>
        <values></values>
        <goodvalue></goodvalue>
        <badvalue></badvalue>
    </colorscheme>
    <colorscheme>
        <attribute>Score</attribute>
        <class>ActivityScheme</class>
        <values></values>
        <goodvalue></goodvalue>
        <badvalue></badvalue>
    </colorscheme>
    <vertexstroke>
        <attribute>Health</attribute>
        <values>0</values>
    </vertexstroke>
    <activitycolor>
        <attribute>Health</attribute>
        <value>0</value>
        <vercolor>
            <r>255</r>
            <g>180</g>
            <b>180</b>
        </vercolor>
    </activitycolor>


</config>
