<?xml version="1.0" encoding="UTF-8"?>
<!--
To change this template, choose Tools | Templates
and open the template in the editor.
-->


<config
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xsi:noNamespaceSchemaLocation='config.xsd'>
    <default_layout>SpatialLayout</default_layout>
    <vertexSize>30</vertexSize>
    <vertexAttributeFilter>Label</vertexAttributeFilter>
    <layoutAxis_X>ObjectPosition_X</layoutAxis_X>
    <layoutAxis_Y>ObjectPosition_Z</layoutAxis_Y>
    <imageLocation>/images/CarTutorialMap.png</imageLocation>
    <imageOffset_X>-133</imageOffset_X>
    <imageOffset_Y>-136</imageOffset_Y>
    <spatialLayoutPosition>600.3503</spatialLayoutPosition>
    <zoomLevel>0</zoomLevel>
    <temporalLayoutbackbone>Player</temporalLayoutbackbone>
    <temporalLayoutscale>0.5</temporalLayoutscale>
    <edgetype>
        <edge>Neutral</edge>
        <edgestroke>MAX</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
    <edgetype>
        <edge>None</edge>
        <edgestroke>MAX</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
    <edgetype>
        <edge>Speed</edge>
        <edgestroke>MAX</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
    <edgetype>
        <edge>Time</edge>
        <edgestroke>MAX</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
    <edgetype>
        <edge>TurnRate</edge>
        <edgestroke>MAX</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
    <edgetype>
        <edge>Flying</edge>
        <edgestroke>MEAN</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
    <edgetype>
        <edge>Landing</edge>
        <edgestroke>MEAN</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
    <edgetype>
        <edge>Flip</edge>
        <edgestroke>MEAN</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
    <edgetype>
        <edge>LostControl</edge>
        <edgestroke>MEAN</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
    <edgetype>
        <edge>Crash</edge>
        <edgestroke>MEAN</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
    <vertexlabelfilter>
        <vertexlabel>Driving</vertexlabel>
    </vertexlabelfilter>
    <vertexlabelfilter>
        <vertexlabel>ChangedGear</vertexlabel>
    </vertexlabelfilter>
    <vertexlabelfilter>
        <vertexlabel>Landing</vertexlabel>
    </vertexlabelfilter>
    <colorscheme>
        <attribute>CurrentGear</attribute>
        <class>ActivityScheme</class>
        <trafficLightType>type2</trafficLightType>
        <isInverted>false</isInverted>
    </colorscheme>
    <colorscheme>
        <attribute>Speed</attribute>
        <class>ActivityScheme</class>
        <trafficLightType>type2</trafficLightType>
        <isInverted>false</isInverted>
    </colorscheme>
    <colorscheme>
        <attribute>TurnRate</attribute>
        <class>ActivityScheme</class>
        <trafficLightType>type2</trafficLightType>
        <isInverted>false</isInverted>
        <values></values>
        <goodvalue>15</goodvalue>
        <badvalue>10</badvalue>
    </colorscheme>
    <colorscheme>
        <attribute>CurrentEnginePower</attribute>
        <class>ActivityScheme</class>
        <trafficLightType>type2</trafficLightType>
        <isInverted>false</isInverted>
    </colorscheme>
    <colorscheme>
        <attribute>Throttle</attribute>
        <class>ActivityScheme</class>
        <trafficLightType>type2</trafficLightType>
        <isInverted>false</isInverted>
    </colorscheme>
    <colorscheme>
        <attribute>DeltaTime</attribute>
        <class>ActivityScheme</class>
        <trafficLightType>type2</trafficLightType>
        <isInverted>true</isInverted>
    </colorscheme>
    <colorscheme>
        <attribute>VelocityVector</attribute>
        <class>ActivityVectorScheme</class>
        <trafficLightType>type2</trafficLightType>
        <isInverted>false</isInverted>
    </colorscheme>
    <colorscheme>
        <attribute>AngularVelocity</attribute>
        <class>ActivityVectorScheme</class>
        <trafficLightType>type2</trafficLightType>
        <isInverted>false</isInverted>
    </colorscheme>
    <colorscheme>
        <attribute>DragVector</attribute>
        <class>ActivityVectorScheme</class>
        <trafficLightType>type2</trafficLightType>
        <isInverted>false</isInverted>
    </colorscheme>
    <activitycolor>
        <attribute>Label</attribute>
        <value>Crash</value>
        <vercolor>
            <r>255</r>
            <g>50</g>
            <b>50</b>
        </vercolor>
    </activitycolor>
    <activitycolor>
        <attribute>Label</attribute>
        <value>Scraping</value>
        <vercolor>
            <r>255</r>
            <g>150</g>
            <b>150</b>
        </vercolor>
    </activitycolor>
    <activitycolor>
        <attribute>Label</attribute>
        <value>Flying</value>
        <vercolor>
            <r>153</r>
            <g>51</g>
            <b>255</b>
        </vercolor>
    </activitycolor>
    <activitycolor>
        <attribute>Label</attribute>
        <value>Landing</value>
        <vercolor>
            <r>153</r>
            <g>76</g>
            <b>0</b>
        </vercolor>
    </activitycolor>
    <activitycolor>
        <attribute>Label</attribute>
        <value>Brake</value>
        <vercolor>
            <r>0</r>
            <g>204</g>
            <b>204</b>
        </vercolor>
    </activitycolor>
    <activitycolor>
        <attribute>Label</attribute>
        <value>HandBrake</value>
        <vercolor>
            <r>255</r>
            <g>136</g>
            <b>120</b>
        </vercolor>
    </activitycolor>
    <activitycolor>
        <attribute>Label</attribute>
        <value>Flip</value>
        <vercolor>
            <r>255</r>
            <g>0</g>
            <b>0</b>
        </vercolor>
    </activitycolor>
    <activitycolor>
        <attribute>Label</attribute>
        <value>LostControl</value>
        <vercolor>
            <r>255</r>
            <g>0</g>
            <b>255</b>
        </vercolor>
    </activitycolor>


</config>
