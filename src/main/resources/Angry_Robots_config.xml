<config
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xsi:noNamespaceSchemaLocation='config.xsd'>
    <layoutbackbone>Player</layoutbackbone>
	<layoutAxis_X>ObjectPosition_X</layoutAxis_X>
	<layoutAxis_Y>ObjectPosition_Z</layoutAxis_Y>
	<coordinatesLayoutScale>25</coordinatesLayoutScale>
	<temporalLayoutscale>0.5</temporalLayoutscale>
	<showentitydate>false</showentitydate>
    <edgetype>
        <edge>Neutral</edge>
        <edgestroke>MAX</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
	<edgetype>
        <edge>None</edge>
        <edgestroke>MAX</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
	<edgetype>
        <edge>Health (Player)</edge>
        <edgestroke>MEAN</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
	<edgetype>
        <edge>Health (Enemy)</edge>
        <edgestroke>MEAN</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
	<edgetype>
        <edge>Score</edge>
        <edgestroke>MEAN</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
	<edgetype>
        <edge>Health</edge>
        <edgestroke>MEAN</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
	<edgetype>
        <edge>Unlocked</edge>
        <edgestroke>MEAN</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
	<edgetype>
        <edge>Interacted</edge>
        <edgestroke>MEAN</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
	<edgetype>
        <edge>Respawned</edge>
        <edgestroke>MEAN</edgestroke>
        <collapsefunction>SUM</collapsefunction>
    </edgetype>
    <colorscheme>
        <attribute>Health</attribute>
        <class>ActivityScheme</class>
        <values></values>
        <goodvalue></goodvalue>
        <badvalue></badvalue>
    </colorscheme>
	<colorscheme>
        <attribute>Health</attribute>
        <class>ActivityRestrictedScheme</class>
        <values></values>
        <goodvalue>100</goodvalue>
        <badvalue>0</badvalue>
		<restrictedAttribute>ObjectName</restrictedAttribute>
        <restrictedValue>Player</restrictedValue>
    </colorscheme>
    <colorscheme>
        <attribute>Score</attribute>
        <class>ActivityScheme</class>
        <values></values>
        <goodvalue></goodvalue>
        <badvalue></badvalue>
    </colorscheme>
    <vertexstroke>
        <attribute>ObjectName</attribute>
        <values>Player</values>
    </vertexstroke>
    <activitycolor>
        <attribute>Health</attribute>
        <value>0</value>
        <vercolor>
            <r>255</r>
            <g>180</g>
            <b>180</b>
        </vercolor>
    </activitycolor>
	<activitycolor>
        <attribute>ObjectName</attribute>
        <value>Player</value>
        <vercolor>
            <r>0</r>
            <g>255</g>
            <b>0</b>
        </vercolor>
    </activitycolor>
	<activitycolor>
        <attribute>ObjectName</attribute>
        <value>KamikazeBuzzer</value>
        <vercolor>
            <r>255</r>
            <g>155</g>
            <b>0</b>
        </vercolor>
    </activitycolor>
	<activitycolor>
        <attribute>ObjectName</attribute>
        <value>EnemySpider</value>
        <vercolor>
            <r>255</r>
            <g>0</g>
            <b>155</b>
        </vercolor>
    </activitycolor>
	<activitycolor>
        <attribute>ObjectName</attribute>
        <value>EnemyMech</value>
        <vercolor>
            <r>255</r>
            <g>150</g>
            <b>100</b>
        </vercolor>
    </activitycolor>


</config>
